    def iterate(data):
        firstPointer = data
        if(data == None):
            return False
        nextPointer = data.next
        while(1):

            print(firstPointer,nextPointer)
            if(firstPointer.next == None):
                return False
            else:
                if(firstPointer == nextPointer):
                    return True

            firstPointer = firstPointer.next.next
            nextPointer = nextPointer.next
